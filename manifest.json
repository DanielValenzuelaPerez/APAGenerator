{
    "manifest_version": 2,
    "name": "APA Generator",
    "version": "0.0.1",
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["content_script.js"]
        }
    ],
    "permissions": ["tabs"],
    "browser_action": {
        "default_icon": "popup/icon.png",
        "default_popup": "popup/popup.html",
        "default_title": "APA Generator"
    }
}